{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "update-version",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeVersion": "15.3.0",
      "env": {
        "TS_NODE_PROJECT": "./bin/tsconfig.json",
      },
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "args": ["${workspaceFolder}/bin/versions-update.ts", "--newversion", "0.0.3"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to npm run debug",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    },
    {
      "name": "Debug with SAM CLI",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5859,
      // From the sam init example, it would be "${workspaceRoot}/hello-world"
      "localRoot": "${workspaceRoot}/.serverless_nextjs/",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
    }
  ]
}